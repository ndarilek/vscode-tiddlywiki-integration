{
    "name": "tiddlywiki-integration",
    "displayName": "TiddlyWiki Integration",
    "description": "Run TiddlyWiki in VS Code",
    "repository": "https://github.com/ndarilek/vscode-tiddlywiki-integration",
    "version": "0.0.1+5.1.22",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:tiddlyWikiView"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": [
            {
                "title": "TiddlyWiki Integration",
                "properties": {
                    "tiddlywiki-integration.path": {
                        "type": "string",
                        "description": "Path to TiddlyWiki",
                        "default": "./wiki.html"
                    }
                }
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "container",
                    "title": "TiddlyWiki",
                    "icon": "icons/tiddlywiki.svg"
                }
            ]
        },
        "views": {
            "container": [
                {
                    "id": "tiddlyWikiView",
                    "type": "webview",
                    "name": "TiddlyWiki",
                    "icon": "icons/tiddlywiki.svg"
                }
            ]
        },
        "keybindings": [
            {
                "command": "tiddlyWikiView.focus",
                "key": "Ctrl+Alt+W",
                "mac": "Cmd+Alt+W"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.68.0",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "@vscode/test-electron": "^2.1.3",
        "eslint": "^8.16.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "typescript": "^4.7.2"
    }
}